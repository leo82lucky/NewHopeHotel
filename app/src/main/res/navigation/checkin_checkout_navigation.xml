<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/checkin_checkout_navigation"
    app:startDestination="@id/checkInCheckOutList">
    <fragment
        android:id="@+id/checkInCheckOutList"
        android:name="com.example.newhopehotel.checkInCheckOut.CheckInCheckOutListFragment"
        android:label="fragment_check_in_check_out_list"
        tools:layout="@layout/fragment_check_in_check_out_list">
        <action
            android:id="@+id/action_checkInCheckOutList_to_checkOutFragment"
            app:destination="@id/checkOutFragment" />
        <action
            android:id="@+id/action_checkInCheckOutList_to_checkInFragment"
            app:destination="@id/checkInFragment" />
    </fragment>
    <fragment
        android:id="@+id/checkInFragment"
        android:name="com.example.newhopehotel.checkInCheckOut.CheckInFragment"
        android:label="fragment_check_in"
        tools:layout="@layout/fragment_check_in" />
    <fragment
        android:id="@+id/checkOutFragment"
        android:name="com.example.newhopehotel.checkInCheckOut.CheckOutFragment"
        android:label="fragment_check_out"
        tools:layout="@layout/fragment_check_out" />
</navigation>